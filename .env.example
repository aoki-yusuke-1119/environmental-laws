# MUI Pro ライセンスキー
NEXT_PUBLIC_MUI_LICENSE_KEY=your-license-key-here

# NextAuth.js 設定
NEXTAUTH_URL=http://localhost:3002
NEXTAUTH_SECRET=generate-random-secret-key

# Cayzen OAuth Configuration
OAUTH_ISSUER=http://localhost:5000
NEXT_PUBLIC_OAUTH_ISSUER=http://localhost:5000  # クライアント側用（OAUTH_ISSUERと同じ値）
OAUTH_CLIENT_ID=your-oauth-client-id
OAUTH_CLIENT_SECRET=your-oauth-client-secret

# 許可IPアドレス（カンマ区切り）
# 例: ALLOWED_IPS=123.456.789.0,98.765.432.1,192.168.1.0/24
# 設定しない場合はIP制限なし
